        $(e.delegateTarget).toggleClass("active");
    });

    $("[data-dropdown-wrap]").on("click", "[data-dropdown-item]", function (e) {
        var $this = $(e.currentTarget);
        var wrap = $this.closest("[data-dropdown-inner-wrap]");
        var item = wrap.find("[data-dropdown-item]");
        var mainWrap = $(e.delegateTarget);
        var value = $this.find("[data-dropdown-text]").text();

        mainWrap.removeClass("active");
        item.removeClass("active");
        $this.addClass("active");
        mainWrap.find("[data-dropdown-active-item-text]").text(value);
        mainWrap.find("[data-dropdown-input]").val(value);
    });

