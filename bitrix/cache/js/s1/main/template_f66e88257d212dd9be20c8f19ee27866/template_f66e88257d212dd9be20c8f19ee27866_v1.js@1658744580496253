        slidesToScroll: 1,
        arrows: false,
        dots: true,
        infinite: false,
        centerMode: true,
        speed: 100,
        // centerPadding: '100px',

        variableWidth: true,
        // centerPadding: '10px',
    }).on('afterChange', function (event, slick, currentSlide, nextSlide) {
        var $prevSlide = $(".js--certification-list [data-slick-index='" + (currentSlide - 1) + "']");
        var $nextSlide = $(".js--certification-list [data-slick-index='" + (currentSlide + 1) + "']");
        $(".js--certification-list [data-slick-index]").removeClass("transparent-half");
        if ($prevSlide.length > 0) {
            $prevSlide.addClass("transparent-half");
        }
        if ($nextSlide.length > 0) {
            $nextSlide.addClass("transparent-half");
        }
    });

    // Скрыть под кат
    function hiddingForHide() {
        var $this = $(this);
        var $wrap = $this.closest("[data-hidding-block]").find("[data-hidding-for-hide]");

        var $hideAfterBlockNum = $wrap.data("hidding-for-hide");

        $wrap.find(">:nth-child(" + $hideAfterBlockNum + ") ~*").toggleClass("hide");

    }

    if ($(".js--guarantee-btn").length > 0 && globParam.windowWidth() < globParam.getMediaSize().PHONES) {
        $(".js--guarantee-btn").on("click", function () {
            $(this).toggleClass("active");
            hiddingForHide.call(this);
            changeTextInBtn.call(this);
        });
        $(".js--guarantee-btn").click();

    }

