    function constructProjectsSlider() {
        if ($(".js--projects-slider-wrap .projects-slider__item").length < 2) return;

        var projectsSliderCountBox = $("[data-projects-slider-item-count]");

        $(".js--projects-slider-wrap").on('reinit init ', function (event, slick, currentSlide, nextSlide) {
            addCountToSlider(currentSlide, slick.slideCount, projectsSliderCountBox);

        }).slick({
            slidesToShow: 1,
            slidesToScroll: 1,
            arrows: true,
            dots: false,
            infinite: false,

            responsive: [

                {
                    breakpoint: globParam.getMediaSize().PHONES,
                    settings: {
                        adaptiveHeight: true,
                    }
                },
            ]

        }).on('afterChange', function (event, slick, currentSlide, nextSlide) {
            addCountToSlider(currentSlide + 1, slick.slideCount, projectsSliderCountBox);
        });

    }

/////////////////////////////// works on main start ////////////////////////////////
    function serializeWorksData() {
        var type_remont = $("[name='type_remont']:checked").val();
        if (type_remont === undefined) type_remont = "";

        var type_object = $("[name='type_object']:checked").val();
        if (type_object === undefined) type_object = "";

        var price_min = $("[data-projects-range-slider]").data("ionRangeSlider").old_from;
        var price_max = $("[data-projects-range-slider]").data("ionRangeSlider").old_to;
        return {
            PROPERTY_TYPE_REMONT: type_remont,
            PROPERTY_TYPE_OBJECT: type_object,
            PROPERTY_PRICE_MIN: price_min,
            PROPERTY_PRICE_MAX: price_max,
            AJAX_REQUEST: "Y"
        }
    }

    function projectPriceSliderFinish(data) {
        sendAjaxRequestWorks();
    }

    function sendAjaxRequestWorks() {
        start_preloader();
        var request = $.ajax({
            url: "/local/templates/main/include/main_our_works.php",
            method: "GET",
            data: serializeWorksData(),
            dataType: "html"
        });

        request.done(function (msg) {
            $("#main_works_ajax_result").html(msg);
            constructProjectsSlider();
            $(".projects__count").html($("#ajax_message").html());

            stop_preloader();
        });

        request.fail(function (jqXHR, textStatus) {
            alert("Request failed: " + textStatus);
            stop_preloader();
        });
        return false;
    }

    $("[name='type_remont']").on("change", function () {
        sendAjaxRequestWorks();
    });
    $("[name='type_object']").on("change", function () {
        sendAjaxRequestWorks();
    });

    ////////////////////////////// works on main end ///////////////////////////
    if ($(".js--projects-slider-wrap").length > 0) {

        constructProjectsSlider();

    }

