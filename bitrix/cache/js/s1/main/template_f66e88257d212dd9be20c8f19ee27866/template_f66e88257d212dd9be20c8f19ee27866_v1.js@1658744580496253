        });
    }

    // Выводим количество слайдов
    function addCountToSlider(currentSlide, slideCount, elem) {
        var current;
        if (typeof currentSlide === "undefined") {
            current = "01";
        } else {
            current = currentSlide > 9 ? currentSlide : "0" + currentSlide;
        }
        var allCount = slideCount > 9 ? slideCount : "0" + slideCount;
        elem.text(current + " / " + allCount)
    }

