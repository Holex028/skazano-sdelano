    //Валидация анкеты
    var questionaryTables = $("[data-questionary-table]");

    function tableValidate() {

        questionaryTables.each(function (outerIndex, outerItem) {

            var questionaryTableRows = $(outerItem).find("[data-questionary-table-row]");

            questionaryTableRows.each(function (indx, item) {
                var questionaryTableItems = $(item).find("[data-questionary-table-item]").find("[data-questionary-table-input]");
                var questionaryTableEmptyItems = questionaryTableItems.filter(function (index) {
                    return $(this).val() == "";
                });

                if (questionaryTableEmptyItems.length == 0) {
                    $(this).closest("[data-questionary-table]").find("[data-questionary-table-input]").each(function (innerIndx, inetItem) {
                        $(inetItem).removeAttr("required");
                    });
                }

            });

        });

    }

    function questionaryInputValidate() {
        var questionaryInputValids = $("[data-questionary-row-valid]").find("[data-questionary-input-valid]");
        questionaryInputValids.each(function (indx, item) {
            if ($(this).val() != "" || $(this).prop('checked')) {
                $(this).closest("[data-questionary-row-valid]").find("[data-questionary-input-valid]").not(this).removeAttr("required");
            }

        });

    }

    $("[data-questionary-btn]").on("click", function () {
        tableValidate();
        questionaryInputValidate();

    });

    // Добавляем нвоые строки в таблицы анкеты соискателя

    $("[data-questionary-table-body]").on("input", "[data-questionary-table-input='clone']", function (e) {
        var $this = $(e.currentTarget);
        var wrap = $this.closest("[data-questionary-table-row]");
        var clone = wrap.clone();

        clone.find("[data-questionary-table-input]").each(function (indx, item) {
            $(item).val("");


            if (wrap.next().length == 0 && $this.val() !== "") {

                var name = $(item).attr("name");
                var nameNumberPosition = name.indexOf("[") + 1;
                var nameNextNumber = +name[nameNumberPosition] + 1;

                var startNewName = name.slice(0, nameNumberPosition);
                var endNewName = name.slice(nameNumberPosition + 1);
                var newName = startNewName + nameNextNumber + endNewName;

                $(item).attr("name", newName);

            }

        });

        if (wrap.next().length == 0 && $this.val() !== "") {
            $(e.delegateTarget).append(clone);
        }

    });

    $('.js--questionary-form').on('submit', function(e) {
        e.preventDefault();
        var $this = $(this);
        var data = $this.serialize();
        // data = data + '&action=ajax';

        $.ajax({
            type: "POST",
            url: "/ajax/questionary.php",
            // data: {"questionary_form": $this.html()},
            data: data,
            dataType: "html",
            success: function(result) {
                $this.addClass("success");
            }
        });
    });


    $("[data-questionary-table-input=\"pc\"]").on("input", function(){
        var $this = $(this);
        var tableInputSiblings = $this.closest("[data-questionary-table-row=\"pc\"]").find("[data-questionary-table-body=\"pc\"]").not($this);
        if($this.val() !== "") {
            tableInputSiblings.removeAttr("required");
            $this.closest("[data-questionary-table-row=\"pc\"]").find("[data-questionary-table-input=\"pc\"]").not($this).val(" ");
        }
    });

    $("[data-input-name]").on("input", function() {
        $(this).val($(this).val().replace(/[0-9]/g, ''));

    });


    if ($(".js--offering-item-btn").length > 0 ) {
        $(".js--offering-item-btn").on("click", function () {
            $(this).toggleClass("active");
            hiddingForHide.call(this);
            changeTextInBtn.call(this);
            $(this).closest("[data-hidding-block]").toggleClass("active");
        });
        $(".js--offering-item-btn").click();

    }

    $("body").on("click", "[href='#modal-form']", function () {
        var modalForm =  $("#modal-form").find("form");
        if(modalForm.hasClass("success")) {
            modalForm.removeClass("success");
        }
    });

    $('[data-reviews-iframe]').iframeTracker({
        blurCallback: function(event) {


            // Do something when iframe is clicked (like firing an XHR request)
            // You can k
            // now which iframe element is clicked via this._overId


            $("[data-reviews-iframe]").not("#" + this._overId).each(function() {
                $(this)[0].contentWindow.postMessage('{"event":"command","func":"pauseVideo","args":""}', '*')
            });
        },
        overCallback: function(element, event) {
            this._overId = $(element).attr('id'); // Saving the iframe wrapper id
        },
        outCallback: function(element, event) {
            this._overId = null; // Reset hover iframe wrapper id
        },
        _overId: null
    });


}

document.addEventListener("DOMContentLoaded", DOMready);


function newcalcDOMready() {
    if (document.querySelector('.calc-new')) {
        const $areaCreditBox = $("[data-newcalc-area-slider]");
        const min = $areaCreditBox.data("min");
        const max = $areaCreditBox.data("max");
        const start = document.querySelector('.calc-new__line input[name="area"]').value;
        $areaCreditBox.ionRangeSlider({
            type: "single",
            min: min,
            max: max,
            from: start,
            hide_min_max: true,
            onFinish: function (data) {
                document.querySelector('.calc-new__line input[name="area"]').value = data.from
                newcalc_calculate_prices()
            },
            postfix: " м²"
        });
        document.querySelectorAll('.calc-new__line select').forEach(select => {
            select.onchange = newcalc_calculate_prices
        })
        document.querySelectorAll('.calc-new__line input').forEach(input => {
            input.onchange = newcalc_calculate_prices
        })
        newcalc_calculate_prices()

        document.addEventListener('submit', function (event) {
            if (event.target.classList.contains('newcalc-modal-form')) {
                event.preventDefault();
                let url = '/ajax/sendnewcalcform.php';
                fetch(url, {
                    method: 'POST',
                    body: JSON.stringify(Object.fromEntries(new FormData(event.target))),
                    headers: {
                        'Content-type': 'application/json; charset=UTF-8'
                    }
                }).then(function (response) {
                    if (response.ok) {
                        return response.text();
                    }
                    return Promise.reject(response);
                }).then(function (data) {
                    console.log(1111111111);
                    if (data == '-2')
                        alert('Необходимо дать согласие на обработку персональных данных!');
                    else if (data == '-1')
                        alert('Укажите, пожалуйста, телефон');
                    else if (data == '-3') {
                        alert('Сработала антиспам защита. Введите проверочный код и отправьте форму еще раз.');
                        reload();
                    } else if (data == '0')
                        alert('При отправке сообщения произошла ошибка. Пожалуйста, обновите страницу и попробуйте еще раз.');
                    else
                        event.target.classList.add('success')
                }).catch(function (error) {
                    console.warn(error);
                });
            }
            if (event.target.classList.contains('price-modal-form')) {
                event.preventDefault();
                let url = '/ajax/sendpriceform.php';
                fetch(url, {
                    method: 'POST',
                    body: JSON.stringify(Object.fromEntries(new FormData(event.target))),
                    headers: {
                        'Content-type': 'application/json; charset=UTF-8'
                    }
                }).then(function (response) {
                    if (response.ok) {
                        return response.text();
                    }
                    return Promise.reject(response);
                }).then(function (data) {
                    if (data == '-2')
                        alert('Необходимо дать согласие на обработку персональных данных!');
                    else if (data == '-1')
                        alert('Укажите, пожалуйста, телефон');
                    else if (data == '-3') {
                        alert('Сработала антиспам защита. Введите проверочный код и отправьте форму еще раз.');
                        reload();
                    } else if (data == '0')
                        alert('При отправке сообщения произошла ошибка. Пожалуйста, обновите страницу и попробуйте еще раз.');
                    else
                        event.target.classList.add('success')
                }).catch(function (error) {
                    console.warn(error);
                });
            }
        });
    }
}

function newcalc_calculate_prices() {
    const area = document.querySelector('.calc-new input[name="area"]').value
    let sum = area
    let days = area
    let params = ''

    params += 'Тип ремонта: ' + document.querySelector('.calc-new input[name="repair-type"]:checked').value + '<br>'
    params += 'Тип недвижимости: ' + document.querySelector('.calc-new input[name="house-type"]:checked').value + '<br>'

    params += 'Площадь помещения по полу: ' + document.querySelector('.calc-new input[name="area"]').value + ' м²<br>'

    params += 'Количество комнат: ' + document.querySelector('.calc-new input[name="rooms"]:checked').value + '<br>'
    params += 'Высота потолков: ' + document.querySelector('.calc-new input[name="height"]:checked').value + '<br>'

    document.querySelectorAll('.calc-new__line input[type="radio"]:checked').forEach(input => {
        sum *= input.dataset.price
        days *= input.dataset.days
    })

    document.querySelectorAll('.calc-new__line input[type="checkbox"]:checked').forEach(input => {
        if (input.name == 'redevelopment')
            sum = parseInt(sum, 10) + parseInt(input.dataset.price, 10)
        else if (input.name == 'loggia')
            sum *= input.dataset.price
        else
            sum = parseInt(sum, 10) + input.dataset.price * area
        days *= input.dataset.days
    })


    document.querySelectorAll('.calc-new__line input[type="checkbox"]').forEach(input => {
        if (input.checked) {
            params += input.nextElementSibling.innerHTML + ': Да<br>'
        } else {
            params += input.nextElementSibling.innerHTML + ': Нет<br>'
        }
    })

    days = Math.ceil(days)
    sum = Math.round(sum)
    const sum2 = Math.round(sum*0.3)
    const sum3 = sum + sum2
    let days2 = days + 9
    document.querySelector('.calc-new__result').innerHTML = `от ${days} до ${days2} ` + getWordForms(days2, 'дня', 'дней', 'дней')
    document.querySelector('.calc-new__result_sum').innerHTML = number_format(sum, 0, '', ' ') + ' ₽'
    document.querySelector('.calc-new__result_sum2').innerHTML = number_format(sum2, 0, '', ' ') + ' ₽'
    document.querySelector('.calc-new__result_sum3').innerHTML = number_format(sum3, 0, '', ' ') + ' ₽'
    document.querySelector('#calc-popup-form [name="request"]').value = params
}

function getWordForms(number,form1,form2,form3)
{
    if (number<0) number = -number;
    if (number%10==1&&number%100!=11) number = form1;
    else if (number%10>=2&&number%10<=4&&(number%100<=4||number%100>=20)) number = form2;
    else number = form3;
    return number;
}

function number_format(number, decimals, dec_point, thousands_sep) {
    // Strip all characters but numerical ones.
    number = (number + '').replace(/[^0-9+\-Ee.]/g, '');
    var n = !isFinite(+number) ? 0 : +number,
        prec = !isFinite(+decimals) ? 0 : Math.abs(decimals),
        sep = (typeof thousands_sep === 'undefined') ? ',' : thousands_sep,
        dec = (typeof dec_point === 'undefined') ? '.' : dec_point,
        s = '',
        toFixedFix = function (n, prec) {
            var k = Math.pow(10, prec);
            return '' + Math.round(n * k) / k;
        };
    // Fix for IE parseFloat(0.55).toFixed(0) = 0;
    s = (prec ? toFixedFix(n, prec) : '' + Math.round(n)).split('.');
    if (s[0].length > 3) {
        s[0] = s[0].replace(/\B(?=(?:\d{3})+(?!\d))/g, sep);
    }
    if ((s[1] || '').length < prec) {
        s[1] = s[1] || '';
        s[1] += new Array(prec - s[1].length + 1).join('0');
    }
    return s.join(dec);
}

function initCalc(){
        /* ------------------- calculation ----------------------- */

    var calcStepCounter = 1;
    var defaultStep = 1;

    var calcLineInner = $("[data-calc-inner-line]");
    var calcBtnPrev = $("[data-calc-btn='prev']");
    var calcBtnNext = $("[data-calc-btn='next']");
    var calcActiveStep = $("[data-calc-bottom-step]");
    var calcBottom = $("[data-calc-bottom]");

    var calcBtnReturn = $("[data-calc-btn-return]");

    var calcStepsItems = $("[data-calc-main]").find("[data-calc-step]");
    var calcStepsCountLength = calcStepsItems.length;


    $("[data-calc-bottom-all]").text(calcStepsCountLength);

    function calculation(operation) {

        if (globParam.windowWidth() < globParam.getMediaSize().PHONES) {

            var offset = 20;
            $('html, body').animate({
                scrollTop: $("#calc").find("[data-calc-line]").offset().top - offset
            }, 1000);

        }

        if (operation === "+") {
            if (calcStepCounter < calcStepsCountLength) {
                calcStepCounter++;

            } else {
                return;
            }
        } else if (operation === "-") {
            if (calcStepCounter > 1) {
                calcStepCounter--;

            } else {
                return;
            }
        } else if (operation === "default") {
            calcStepCounter = 1;
        }

        if (calcStepCounter === calcStepsCountLength) {
            calcBottom.addClass("hide");

        } else {
            if (calcBottom.hasClass("hide")) {
                calcBottom.removeClass("hide");
            }
        }

        calcActiveStep.text(calcStepCounter);
        calcStepsItems.removeClass("active");
        $("[data-calc-step='" + calcStepCounter + "']").addClass("active");

        calcLineInner.css("width", (100 / (+calcStepsCountLength - defaultStep) * (+calcStepCounter - defaultStep)) + "%");

        $("[data-calc-dot='" + calcStepCounter + "']").addClass("active");
        $("[data-calc-dot='" + calcStepCounter + "'] ~ [data-calc-dot]").removeClass("active");

    }

    calcBtnNext.on("click", function (e) {
        e.preventDefault();

        if (calcStepCounter == 4) {
            start_preloader();
            var calc_type_property = $("[name='calc_type_property']:checked").val();
            var calc_type_remont = $("[name='calc_type_remont']:checked").val();
            var calc_walls = $("[name='calc_walls']:checked").val();
            var calc_rooms = $("[name='calc_rooms']:checked").val();
            var calc_space = $("[name='calc_space']").val();
            var calc_height = $("[name='calc_height']").next(".dropdown__active-item").find(".dropdown__active-item-text").html();

            var height_kostil = {'2,5':'height_25','2,7':'height_27','3':'height_3','3,5':'height_35'}
//console.log(height_kostil[calc_height]);
//console.log("calculator_data", calculator_data);
//console.log("calculator_data[calc_type_remont]", calculator_data[calc_type_remont]);
//console.log("height_kostil[calc_height]", height_kostil[calc_height]);
//console.log("calc_height", parseFloat(calculator_data[calc_type_remont][height_kostil[calc_height]] ));

            var calc_result_works = parseFloat(calculator_data[calc_type_remont]["price"]) * parseFloat(calc_space) * parseFloat(calculator_data[calc_type_remont][calc_type_property]) * parseFloat(calculator_data[calc_type_remont][height_kostil[calc_height]]);
            if (calc_walls) {
                calc_result_works = calc_result_works * parseFloat(calculator_data[calc_type_remont]["walls"]);
            }
            var calc_result_materials = calc_result_works * parseFloat(calculator_data[calc_type_remont]["ЧМ"]);
            var calc_result_summ = calc_result_works + calc_result_materials;

            $("#calc_result_works").html(calc_result_works.toLocaleString('ru-RU') + "  руб.");
            $("#calc_result_materials").html(calc_result_materials.toLocaleString('ru-RU') + "  руб.");
            $("#calc_result_summ").html(calc_result_summ.toLocaleString('ru-RU') + "  руб.");

            setTimeout(stop_preloader_calculation, 500);
        } else {
            calculation("+");
        }
    });

    function stop_preloader_calculation() {
        stop_preloader();
        calculation("+");
    }

    calcBtnPrev.on("click", function (e) {
        e.preventDefault();
        calculation("-");
    });

    calcBtnReturn.on("click", function () {
        calculation("default");
    });
       $("#calc_form").submit(function(){
        if(checkInputPhone($(this))) {
            sendFormToCRM($(this));
        }
        return false;
    });
}
document.addEventListener("DOMContentLoaded", newcalcDOMready);
/* End */
;
; /* Start:"a:4:{s:4:"full";s:33:"/local/ajax/init.js?1658413995682";s:6:"source";s:19:"/local/ajax/init.js";s:3:"min";s:0:"";s:3:"map";s:0:"";}"*/
$(document).ready(function(){if ($('#calc').hasClass('ajax')) {

    var request = $.ajax({
        url: '/local/ajax/calculator.php',
        method: 'GET'
    });
    request.done(function (response) {
        $('#calc').html(response);
        $(".handler-buttons__item_pos_1").show();
        $('#calc').removeClass('ajax');
        initCalc();

    });
}
});

$(document).ready(function(){if ($('#text-reviews').hasClass('ajax')) {
    var request = $.ajax({
        url: '/local/ajax/text-reviews.php',
        method: 'GET'
    });
    request.done(function (response) {
        $('#text-reviews').html(response);
        $('#text-reviews').removeClass('ajax');
    });
}
});
/* End */
;
; /* Start:"a:4:{s:4:"full";s:89:"/local/templates/main/components/sotbit/regions.choose/location/script.js?164849389911358";s:6:"source";s:73:"/local/templates/main/components/sotbit/regions.choose/location/script.js";s:3:"min";s:0:"";s:3:"map";s:0:"";}"*/
class RegionsChoose {

    constructor() {

        this.getRegion();

        this.rootId = 'regions_choose_component';
        this.rootDropDownId = 'regions_choose_component_dropdown';
        this.selectRegionID = 'regon_choose_select-city__modal';
        this.selectRegionOverlayID = 'regon_choose_modal__overlay';
    
        this.questionRegionId = null;
        this.currentRegionId = null;
        this.loctionList = null;

        this.bigCityList = null;
        this.countryList = null;
        this.activCountry = null;
        this.defoultExampleRegion = null;


        this.root = document.getElementById(this.rootId);
        this.rootDropDown = document.getElementById(this.rootDropDownId);
        this.selectRegion = document.getElementById(this.selectRegionID);

        this.setEvents();
    }

    getEntity(parent, entity, all=false) {
        if (!parent || !entity) {
            return null;
        }
        if (all) {
            return parent.querySelectorAll('[data-entity="' + entity + '"]');
        }
        return parent.querySelector('[data-entity="' + entity + '"]');
    }

    setEvents() {
        const yesBtn = this.getEntity(this.rootDropDown, 'select-city__dropdown__choose__yes');
        const notBnt = this.getEntity(this.rootDropDown, 'select-city__dropdown__choose__no');
        const cityName = this.getEntity(this.root, 'select-city__block__text-city');
        const selectRegionClose = this.getEntity(this.selectRegion, 'select-city__close');
        const searchLine = this.getEntity(this.selectRegion, 'select-city__modal__submit__input');

        this.defoultExampleRegion = this.getEntity(this.selectRegion, 'select-city__input__example').cloneNode(true);

        yesBtn.addEventListener('click', () => this.onSetRegion(this.questionRegionId));
        notBnt.addEventListener('click', () => this.onShowALLRegions());
        cityName.addEventListener('click', () => this.onShowALLRegions());
        selectRegionClose.addEventListener('click', () => this.onSelectRegionShow(false));
        searchLine.addEventListener('input', e => this.onInputSearch(e));

    }

    onSetRegion(regionId) {
        BX.ajax.runAction('sotbit:regions.ChooseComponentController.setRegion', {
            data: {regionId: regionId},
        }).then(
            (res) => res.data.actions.forEach(i => this[i](res.data)),
            (err) => {console.log(err)},
        )
    }

    dropDownShow(action) {
        if (action) {
            this.rootDropDown.setAttribute('style', 'display: block;');
        } else {
            this.rootDropDown.setAttribute('style', 'display: none;');
        }
    }

    onShowALLRegions(countryId=Number.MAX_SAFE_INTEGER) {

        if (this.loctionList !== null && countryId === Number.MAX_SAFE_INTEGER) {
            return this.SHOW_SELECT_REGIONS({
                allRegions: this.loctionList,
                locationTemplateData: {activ: this.activCountry}
            });
        }

        BX.ajax.runAction('sotbit:regions.ChooseComponentController.showLocations', {
            data: {countryId}
        })
            .then(
                (res) => res.data.actions.forEach(i => this[i](res.data)),
                (err) => console.log(err),
            )
    }

    getRegion(deleteCookies=false) {

        const query = new URLSearchParams(window.location.search);

        const params = deleteCookies
            ? {deletCookies: 1}
            : {redirectRegionId: query.get('redirectRegionId')}

        this.removeRegionGetParams(query);

        BX.ajax.runAction('sotbit:regions.ChooseComponentController.getRegion', {
            data: params,
        })
        .then(
            (res) => res.data.actions.forEach(i => this[i](res.data)),
            (err) => console.log(err),
        )

    }

    onSelectRegionShow(action) {
        if (action) {
            this.selectRegion.setAttribute('style', 'display: block;');
        } else {
            this.selectRegion.setAttribute('style', 'display: none;');
        }
    }

    SHOW_REGION_NAME ({currentRegionName}) {
        const elment = this.getEntity(this.root, 'select-city__block__text-city');
        elment.innerText = currentRegionName;
    }

    SHOW_QUESTION ({currentRegionName, currentRegionId}) {
        this.questionRegionId = currentRegionId;
        this.dropDownShow(true);
        const element = this.getEntity(this.rootDropDown, 'select-city__dropdown__title');
        element.innerText = element.textContent.replace('###', currentRegionName).trim();
    }

    CONFIRM_DOMAIN ({currentRegionName}) {
        this.dropDownShow(false);
        const elemnt = this.getEntity(this.root, 'select-city__block__text-city');
        elemnt.innerText = currentRegionName;
        this.getEntity(this.selectRegion, 'select-city__js').innerText = currentRegionName;
    }

    SHOW_SELECT_REGIONS ({allRegions, locationTemplateData}) {

        this.onSelectRegionShow(true);

        if (locationTemplateData === null) {
            locationTemplateData = {
                bigCity: allRegions,
                country: {},
                activ: 0,
            };
        }

        if (this.activCountry === locationTemplateData['activ']) {
            return;
        }

        this.loctionList = allRegions;
        this.countryList = locationTemplateData['country'];
        this.activCountry = locationTemplateData['activ'];
        this.bigCityList = locationTemplateData['bigCity'];

        this.citysRender();
        this.renderCountryTabs();
    }

    REDIRECT_TO_SUBDOMAIN ({currentRegionCode, currentRegionId}) {
        const hostName = window.location.hostname;
        const protocol = window.location.protocol;
        const newUrl = window.location.href.replace(hostName, currentRegionCode);
        const url = new URL(newUrl, `${protocol}${currentRegionCode}`);
        url.searchParams.set('redirectRegionId', currentRegionId);
        window.location.href = url.toString();
    }

    onInputSearch(e) {
        this.searchLine = e.currentTarget.value;
        this.citysRender();
    }

    citysRender() {

        const bitCityWraper = this.getEntity(
            this.getEntity(this.selectRegion, 'select-city__list_wrapper_favorites'),
            'select-city__list',
        );
        const litersWrapper = this.getEntity(this.selectRegion, 'select-city__list_wrapper_cities');
        const exapmleRegions = this.getEntity(this.selectRegion, 'select-city__input__example');

        Array.from(bitCityWraper.childNodes).forEach(i => i.remove());
        Array.from(litersWrapper.childNodes).forEach(i => i.remove());
        Array.from(exapmleRegions.childNodes).forEach(i => i.remove());
        exapmleRegions.innerHTML = this.defoultExampleRegion.innerHTML;

        if (!!this.bigCityList && Object.keys(this.bigCityList).length !== 0) {
            this.getEntity(this.selectRegion, 'select-city__tab_name_content__big_city')
                .setAttribute('style', 'display: block;');
        } else {
            this.getEntity(this.selectRegion, 'select-city__tab_name_content__big_city')
                .setAttribute('style', 'display: none;');
        }

        Object.keys(this.bigCityList)
            .filter(i => new RegExp(this.searchLine, 'i').test(this.bigCityList[i]))
            .forEach(i => {
                const element = document.createElement('p');
                element.setAttribute('class', 'select-city__list_item');
                element.innerText = this.bigCityList[i];
                element.addEventListener('click', () => {
                    this.onSetRegion(i);
                });
                bitCityWraper.append(element);
            });

        const liters = new Set(
            Object.values(this.loctionList)
                .filter(i => new RegExp(this.searchLine, 'i').test(i))
                .map(i => i[0])
        );

        Array.from(liters).forEach(i => {
            const oneliterWrapper = document.createElement('div')
            oneliterWrapper.setAttribute('class', 'select-city__list_letter_wrapper');

            const literElem = document.createElement('div');
            literElem.setAttribute('class', 'select-city__list_letter');
            literElem.innerText = i;
            oneliterWrapper.append(literElem);

            const citysWrapper = document.createElement('div');
            citysWrapper.setAttribute('class', 'select-city__list');
            Object.keys(this.loctionList)
                .filter(j => i === this.loctionList[j][0])
                .filter(i => new RegExp(this.searchLine, 'i').test(this.loctionList[i]))
                .forEach(cityId => {
                    const cityElem = document.createElement('p');
                    cityElem.setAttribute('class', 'select-city__list_item');
                    cityElem.addEventListener('click', () => {
                        this.onSetRegion(cityId);
                    });
                    cityElem.innerText = this.loctionList[cityId];
                    citysWrapper.append(cityElem);
                });

            oneliterWrapper.append(citysWrapper);
            litersWrapper.append(oneliterWrapper);
        })


        const regionKeys = Object.keys(this.loctionList);
        Array.from(exapmleRegions.querySelectorAll('span')).forEach((item, index) => {
            item.innerText = this.loctionList[regionKeys[index]];
            item.addEventListener('click', () => {
                this.onSetRegion(regionKeys[index]);
            });
        });

    }

    renderCountryTabs() {
        const countryWrap = this.getEntity(this.selectRegion, 'sotbit-regions-tabs');

        if (Array.from(countryWrap.childNodes).filter(i => i instanceof HTMLLIElement).length > 0) {
            return;
        }

        const activClass = 'active';

        Object.keys(this.countryList).forEach(i => {

            const element = document.createElement('li');
            if (this.activCountry === i) {
                element.setAttribute('class', `select-city__tab ${activClass}`);
            } else {
                element.setAttribute('class', 'select-city__tab');
            }

            element.innerText = this.countryList[i];

            element.addEventListener('click', () => {
                if (element.classList.contains(activClass)) {
                    return;
                }

                const newCountryWrap = this.getEntity(this.selectRegion, 'sotbit-regions-tabs');
                    Array.from(newCountryWrap.childNodes).forEach(j =>  {
                        if (j instanceof HTMLLIElement) {
                            j.classList.remove(activClass)
                        }
                    });

                element.classList.add(activClass);
                this.onShowALLRegions(i);

            })

            countryWrap.append(element);

            countryWrap.childNodes.forEach(i => {
                if (!(i instanceof HTMLLIElement)) {
                    return;
                }
            })

        });
    }

    removeRegionGetParams(query) {
        if (query.has('redirectRegionId')) {
            const url = new URL(window.location.href, window.location.href);
            url.searchParams.delete('redirectRegionId');
            window.history.replaceState(null, '', url)
        }
    }
}
/* End */
