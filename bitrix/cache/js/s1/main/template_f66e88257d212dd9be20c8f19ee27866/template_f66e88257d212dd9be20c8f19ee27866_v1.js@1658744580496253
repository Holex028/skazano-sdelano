    var $rangeCreditBox = $("[data-projects-range-slider]");

    var instanceCreditBox;
    var min = $rangeCreditBox.data("min");
    var max = $rangeCreditBox.data("max");

    $rangeCreditBox.ionRangeSlider({
        // skin: "round",
        type: "double",
        min: min,
        max: max,
        hide_min_max: true,
        onFinish: function (data) {
            projectPriceSliderFinish(data);
        },
        onChange: function (data) {
            if(data.from_percent === 100 ) {

                $(data.input[0]).find('.irs-from').addClass("irs-from_pos_right");

            } else {
                if($(data.input[0]).find('.irs-from').hasClass("irs-from_pos_right")) {
                    $(data.input[0]).find('.irs-from').removeClass("irs-from_pos_right");
                }
            }

            if(data.from_percent === 0 && data.to_percent === 0) {
                $(data.input[0]).find('.irs-to').addClass("irs-to_pos_left");

            } else {
                if($(data.input[0]).find('.irs-to').hasClass("irs-to_pos_left")) {
                    $(data.input[0]).find('.irs-to').removeClass("irs-to_pos_left");
                }
            }
            if(data.from_percent > 75) {
                $(data.input[0]).find('.irs-single').addClass("irs-single_pos_right");

            } else {
                if($(data.input[0]).find('.irs-single').hasClass("irs-single_pos_right")) {
                    $(data.input[0]).find('.irs-single').removeClass("irs-single_pos_right");
                }
            }
            if(data.from_percent < 10) {
                $(data.input[0]).find('.irs-single').addClass("irs-single_pos_left");

            } else {
                if($(data.input[0]).find('.irs-single').hasClass("irs-single_pos_left")) {
                    $(data.input[0]).find('.irs-single').removeClass("irs-single_pos_left");
                }
            }
        },
        // postfix: " руб."

    });

    $("[data-projects]").on("click", "[data-projects-slider-img-bottom-box]", function () {
