    var priceServicesTable = $("[data-price-services-table]");

    if (priceServicesTable.length > 0) {

        var priceServicesTableWrap = $("[data-price-services-table-wrap]");

        priceServicesTable.each(function (indx, item) {
            var $item = $(item);

            if (($item[0].scrollWidth - $item[0].clientWidth) > 0) {
                $item.closest("[data-price-services-table-wrap]").addClass("shadow_right");
            }

        });

        priceServicesTable.on("scroll", function () {
            var $this = $(this),
                wrap = $this.closest("[data-price-services-table-wrap]"),
                widthOfScroll = $this[0].scrollWidth - $this[0].clientWidth;
            if ($this.scrollLeft() > 0) {
                wrap.addClass("shadow_left");
            } else if ($this.scrollLeft() <= 0 && wrap.hasClass("shadow_left")) {
                wrap.removeClass("shadow_left");
            }

            if ($this.scrollLeft() >= widthOfScroll) {
                wrap.removeClass("shadow_right");

            } else if ($this.scrollLeft() < widthOfScroll && !wrap.hasClass("shadow_right")) {
                wrap.addClass("shadow_right");
            }

        });

        // Скрываем стандартную полосу прокрутки
        priceServicesTable.css("margin-bottom", "-" + (priceServicesTableWrap[0].offsetHeight - priceServicesTable[0].clientHeight) + "px");

    }

    // Скролл к нужному элементу
    function slowScroll(elemToScroll, offset) {
        var offset = (typeof offset !== "undefined" && offset) ? offset : 0;
        var $elemToScroll = (typeof elemToScroll !== "undefined" && elemToScroll) ? $(elemToScroll) : $(this).data("slow-scroll-to");

        $('html, body').animate({
            scrollTop: $($elemToScroll).offset().top - offset
        }, 1000);
        return false;
    }

    $("[data-slow-scroll-to]").on("click", function (e) {
        e.preventDefault();
        slowScroll.call(this);
    });

    if ($("#contacts-map").length > 0) {

        ymaps.ready(contactsMapinit);

        var contactsMapCenter = [window.sotbit_lat, window.sotbit_lon];

        if (globParam.windowWidth() < globParam.getMediaSize().TABLETS) {
            contactsMapCenter = [window.sotbit_lat -0.009, window.sotbit_lat -0.09];

        }

        if (globParam.windowWidth() < globParam.getMediaSize().PHONES) {
            contactsMapCenter = [window.sotbit_lat + 0.06 , window.sotbit_lat +0.001];

        }

        function contactsMapinit() {
            // Создание карты.
            var myMap = new ymaps.Map("contacts-map", {
                // Координаты центра карты.
                // Порядок по умолчанию: «широта, долгота».
                // Чтобы не определять координаты центра карты вручную,
                // воспользуйтесь инструментом Определение координат.
                center: contactsMapCenter,
                // Уровень масштабирования. Допустимые значения:
                // от 0 (весь мир) до 19.
                zoom: 15,
                controls: []
            });

            var myPlacemark = new ymaps.Placemark([55.635945, 37.437604], {}, {
                iconLayout: 'default#image',
                iconImageHref: '/local/templates/main/img/point-logo.svg',
                // iconImageSize: [60, 70],
                // iconImageOffset: [45, -10]

            });

            myMap.behaviors.disable('scrollZoom');
            myMap.geoObjects.add(myPlacemark);

        }
    }

