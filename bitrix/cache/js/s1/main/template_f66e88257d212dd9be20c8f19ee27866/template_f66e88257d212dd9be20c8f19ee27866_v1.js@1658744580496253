$(document).ready(function(){
    //Если нет блока на странице - скрываем кнопки
    if ($(".calc-new").length > 0){
        $('.handler-buttons__item_pos_1').click(function(e) {
            e.preventDefault();
            slowScroll(".calc-new", 100)
        })
    }else{

    }

    if ($("#main-form").length > 0){

    }else{
        $(".handler-buttons__item_pos_2").hide();
    }

    /// /about/works/ ////
    $(".main").on("click", "#ajax_pagination a", function(){
        start_preloader();
        var page_number = $(this).data("next_page");
        var PROPERTY_TYPE_REMONT = $("#js-select-type-remont li.active span").data("type_id");
        $("#ajax_pagination").remove();
        var request = $.ajax({
            method: "GET",
            data: { PAGEN_1 : page_number, PROPERTY_TYPE_REMONT : PROPERTY_TYPE_REMONT, AJAX_REQUEST: "Y" },
            dataType: "html"
        });

        request.done(function( msg ) {
            $("#ajax_result").append(msg);
            stop_preloader();
        });

        request.fail(function( jqXHR, textStatus ) {
            alert( "Request failed: " + textStatus );
            stop_preloader();
        });
        return false;
    });

    $(".main").on("click", "#js-select-type-remont span", function(){
        start_preloader();
        $this = $(this);
        $this.parents("ul").find("li").removeClass("active");
        $this.parent("li").addClass("active");
        var PROPERTY_TYPE_REMONT = $this.data("type_id");
        var request = $.ajax({
            method: "GET",
            data: { PROPERTY_TYPE_REMONT : PROPERTY_TYPE_REMONT, AJAX_REQUEST: "Y" },
            dataType: "html"
        });

        request.done(function( msg ) {
            $("#ajax_result").html(msg);
            stop_preloader();
        });

        request.fail(function( jqXHR, textStatus ) {
            alert( "Request failed: " + textStatus );
            stop_preloader();
        });
        return false;
    });
    /////////////about/works/ END////////////////////


    $("#modal-form form").submit(function(){
        if(checkInputPhone($(this))) {
            sendFormToCRM($(this));
        }

        return false;
    });

  $("#form-promo-popup form").submit(function(){
        if(checkInputPhone($(this))) {
            sendForm($(this));
        }

        return false;
    });

    $("#contacts-form form").submit(function(){
        if(checkInputPhone($(this))) {
            sendFormToCRM($(this));
        }
        return false;
    });
    $("#main_form_block").submit(function(event){
        if(checkInputPhone($(this))) {
            sendFormToCRM($(this));
        }
        return false;
    });
    $("#call_form_block").submit(function(){
        if(checkInputPhone($(this))) {
            sendFormToCRM($(this));
        }
        return false;
    });
    $("#questions_form").submit(function(){
        if(checkInputPhone($(this))) {
            sendFormToCRM($(this));
        }
        return false;
    });
 




});

// FORMS

$('form').each(function () {
    $(this).find('input[type=button], input[type=submit], button').click(function () {
        console.log(11111);
        var valid = true;
        var $form = $(this).parents('form');

        if (valid) {
            console.log(147);
            var request = $.ajax({
                url: '/local/templates/main/include/b24sendform.php',
                method: 'POST',
                data: data
            });


            request.done(function( response ) {
                // setTimeout(stop_preloader, 500);
                if (response.error == 201){
                    $form.addClass("success");
                }else{
                    alert (response.error + " " + response.error_message);
                }
                stop_preloader();
            });

            request.fail(function( jqXHR, textStatus ) {
                alert( "Request failed: " + textStatus );
                stop_preloader();
            });

        }
        return false;
    });
});


function sendFormToCRM(form){


    $form = form;
    var data = $form.serialize();

    start_preloader($form);

    var request = $.ajax({
        url: '/local/templates/main/include/b24sendform.php',
        method: 'POST',
        data: data,
        dataType: 'JSON'
    });


    request.done(function( response ) {
        console.log(response);
        // setTimeout(stop_preloader, 500);
        if (response.error == 201){
            $form.addClass("success");
        }else{
            alert (response.error + " " + response.error_message);
        }
       stop_preloader();
    });

    request.fail(function( jqXHR, textStatus ) {
        alert( "Request failed: " + textStatus );
        stop_preloader();
    });
}

// FORMS
function sendForm(form){
    console.log(222222);
    $form = form;
    start_preloader($form);
    var request = $.ajax({
        url: "/local/templates/main/include/sendform.php",
        method: "GET",
        data: $form.serialize(),
        dataType: "JSON"
    });
    request.done(function( response ) {
        // setTimeout(stop_preloader, 500);
        if (response.error == 201 ){
            $form.addClass("success");
        }else{
            alert (response.error + " " + response.error_message);
        }
        stop_preloader();
    });

    request.fail(function( jqXHR, textStatus ) {
        alert( "Request failed: " + textStatus );
        stop_preloader();
    });



}

function checkInputPhone ($this) {

    var phoneInput = $this.find("[data-input-phone]");
    var phoneInputClearValue = phoneInput.val().replace(/[^0-9]/gim,'');
    if(phoneInputClearValue.length != 11) {
        phoneInput.addClass("phone-error");
        return false;
    } else {
        if(phoneInput.hasClass("phone-error")) {
            phoneInput.removeClass("phone-error");
        }
        return true;
    }



}


function start_preloader(container) {
    stop_preloader();
    if (container === undefined)
        container = "body";
// $(container).css({"position": "relative"});
    $(container).append('<div id="loader-wrapper"><div id="loader"></div></div>');
    if (container !== "body") {
        $("#loader-wrapper").css({"position": "absolute"});
    }

}
function stop_preloader() {
    $("#loader-wrapper").remove();
}

window.onRoistatAllModulesLoaded = function() {
    window.roistat.leadHunter.onAfterSubmit = function(leadData) {
        ym(55724113,'reachGoal','ALL_CALLS');
    }
};


function setCookie(name,value,days) {
    var expires = "";
    if (days) {
        var date = new Date();
        date.setTime(date.getTime() + (days*24*60*60*1000));
        expires = "; expires=" + date.toUTCString();
    }
    document.cookie = name + "=" + (value || "")  + expires + "; path=/";
}
function getCookie(name) {
    var nameEQ = name + "=";
    var ca = document.cookie.split(';');
    for(var i=0;i < ca.length;i++) {
        var c = ca[i];
        while (c.charAt(0)==' ') c = c.substring(1,c.length);
        if (c.indexOf(nameEQ) == 0) return c.substring(nameEQ.length,c.length);
    }
    return null;
}

jQuery(function() {
    jQuery('.js-show-more-reviews').click(function() {
        let parent = jQuery(this).parent().find('.reviews');
        if (parent.hasClass('active')) {
            parent.removeClass('active');
            jQuery(this).text('Показать больше');
        } else {
            parent.addClass('active');
            jQuery(this).text('Свернуть');
        }
        return false;
    });
    var fixed_wrapper = document.querySelector('.main-wrapper');
    window.onscroll = function() {
      if (window.pageYOffset > 0) {
        fixed_wrapper.classList.add("fixed-header");
      } else {
        fixed_wrapper.classList.remove("fixed-header");
      }
    };
    window.scroll()

    jQuery('.post-tag').each(function() {
        let parent = $(this)
        if (parent.height() > 40) {
            parent.after('<div class="post-tag-more"><a href="#" class="btn">Показать больше</a></div>');
            $('.post-tag-more').click(function() {
                parent.toggleClass('post-tag_cutted')
                if ($(this).find('.btn').text() == 'Показать больше') 
                    $(this).find('.btn').text('Свернуть') 
                else 
                    $(this).find('.btn').text('Показать больше')
                return false;
            });
            parent.addClass('post-tag_cutted');
        }
    });
})

function slowScroll(elemToScroll, offset) {
    var offset = (typeof offset !== "undefined" && offset) ? offset : 0;
    var $elemToScroll = (typeof elemToScroll !== "undefined" && elemToScroll) ? $(elemToScroll) : $(this).data("slow-scroll-to");

    $('html, body').animate({
        scrollTop: $($elemToScroll).offset().top - offset
    }, 1000);
    return false;
}

