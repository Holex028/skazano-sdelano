    function showHideOnScroll($block, height) {
        if ($(window).scrollTop() > height) {
            $block.addClass('slow-show ');
        } else {
            $block.removeClass('slow-show');
        }
    }

    var btnToTop = $('[data-btn-to-top]');
    btnToTop.on('click', function (e) {
        e.preventDefault();
        $('html, body').animate({scrollTop: 0}, '300');
    });

    // Табы
    function tabsStart(e) {
        e.preventDefault();
        e.stopPropagation();

        var $this = $(this),
            target = $this.attr('href');

        $this.closest("[data-tabs-nav-list]").find("[data-tabs-nav-item].active").removeClass("active");
        $this.closest("[data-tabs-nav-item]").addClass("active");

        $(e.delegateTarget).find("[data-tabs-tab].active").removeClass("active");
        $(e.delegateTarget).find(target).addClass("active");
    }

    $(".js--offering").on("click", "[data-tabs-toggle]", function (e) {
        tabsStart.call(this, e);
    });

    $(".js--price-services").on("click", "[data-tabs-toggle]", function (e) {
        tabsStart.call(this, e);
    });

    $(".js--repairs-use-content").on("click", "[data-tabs-toggle]", function (e) {
        tabsStart.call(this, e);
    });

    $(".js--our-work-variant-wrap").on("click", "[data-tabs-toggle]", function (e) {
        tabsStart.call(this, e);
    });

    $(".js--type-repair-tabs").on("click", "[data-tabs-toggle]", function (e) {
        tabsStart.call(this, e);
    });

    if ($("[data-offering-decor-text]").length > 0 && globParam.windowWidth() > globParam.getMediaSize().PHONES) {

        var $thisDecor1 = $("[data-offering-decor-text='1']");
        var $thisDecor2 = $("[data-offering-decor-text='2']");

        $("[data-offering-nav-item-decor]").on("click", function () {
            var $this = $(this);
            var $activeCount = $this.data("offering-nav-item-decor");
            if ($activeCount === 1) {
                $thisDecor1.removeClass("hide");
                $thisDecor2.addClass("hide");

            } else if ($activeCount === 2) {
                $thisDecor1.addClass("hide");
                $thisDecor2.removeClass("hide");
            }

        })

    }

